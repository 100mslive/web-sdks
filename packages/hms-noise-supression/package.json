{
  "version": "0.1.7-alpha",
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "concurrently \"yarn dev\" \"yarn types\"",
    "dev": "node ../../scripts/dev",
    "build:only": "node ../../scripts/build",
    "build": "yarn build:only && yarn types:build",
    "types": "tsc -w",
    "types:build": "tsc -p tsconfig.build.json",
    "test": "jest --maxWorkers=1",
    "lint": "eslint -c ../../.eslintrc .",
    "lint:fix": "yarn lint --fix",
    "prepare": "yarn build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "format": "prettier --write src/**/*.ts"
  },
  "peerDependencies": {
    "@100mslive/hms-video": "0.1.44"
  },
  "name": "@100mslive/hms-noise-suppression",
  "author": "vishaldhull09",
  "module": "dist/index.js",
  "size-limit": [
    {
      "path": "dist/index.cjs.js",
      "limit": "100 KB"
    },
    {
      "path": "dist/index.js",
      "limit": "100 KB"
    }
  ],
  "devDependencies": {
    "@100mslive/hms-video": "0.1.44"
  }
}
