    "@rollup/plugin-typescript": "^8.3.1",
    "rollup": "^2.70.1",
    "rollup-plugin-esbuild": "^4.9.3",
    "rollup-plugin-terser": "^7.0.2",
    "vue": "^3.4.38"
  },
  "scripts": {
    "prestart": "rm -rf dist",
    "start": "rollup -c -w",
