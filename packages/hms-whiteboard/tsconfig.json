{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "target": "ES2020",
    "useDefineForClassFields": true,
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "outDir": "dist",
    "composite": false,
    "incremental": false,
    "declarationMap": false,
    "sourceMap": false,
    "emitDeclarationOnly": true,
    "noImplicitReturns": false,
    "noUnusedParameters": false
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}
