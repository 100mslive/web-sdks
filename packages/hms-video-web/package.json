{
  "name": "@100mslive/hms-video",
  "version": "0.1.35-alpha.13",
  "license": "UNLICENSED",
  "main": "dist/hms-video.cjs.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "prestart": "rm -rf dist && yarn types:build",
    "start": "concurrently 'yarn dev' 'yarn types'",
    "dev": "node ./scripts/dev",
    "build:only": "node ./scripts/build",
    "build": "node ./scripts/build && yarn types:build",
    "types": "tsc -w",
    "types:build": "tsc -p tsconfig.build.json && tsconfig-replace-paths -p tsconfig.build.json",
    "test": "jest --maxWorkers=1",
    "lint": "eslint -c ../../.eslintrc .",
    "lint:fix": "yarn lint --fix",
    "prepare": "yarn build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "format": "prettier --write src/**/*.ts"
  },
  "author": "100ms <tech-common@100ms.live>",
  "module": "dist/hms-video.esm.js",
  "devDependencies": {
    "@types/sdp-transform": "^2.4.4",
    "@types/ua-parser-js": "^0.7.36",
    "@types/uuid": "^8.3.0",
    "jest-canvas-mock": "^2.3.1",
    "tsconfig-replace-paths": "^0.0.11",
    "tslib": "^2.2.0"
  },
  "dependencies": {
    "eventemitter2": "^6.4.5",
    "sdp-transform": "^2.14.1",
    "ua-parser-js": "^1.0.1",
    "uuid": "^8.3.2",
    "webrtc-adapter": "^8.0.0"
  }
}
