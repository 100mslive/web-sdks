{
  "name": "@100mslive/react-icons",
  "source": "src/index.tsx",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "version": "0.0.11",
  "author": "100ms",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "svg-build": "svgr --icon --typescript --replace-attr-values '#fff=currentColor' --svg-props width='24px' --svg-props height='24px' -d src assets && yarn format",
    "build:only": "yarn svg-build && rollup -c",
    "types": "tsc -p tsconfig.json",
    "build": "yarn build:only && yarn types",
    "format": "prettier -w src/**",
    "lint": "eslint",
    "lint:fix": "eslint src/** --fix"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@svgr/cli": "^5.5.0",
    "@types/react": "^17.0.38",
    "react": "^17.0.2",
    "rollup": "^2.70.1",
    "rollup-plugin-esbuild": "^4.9.1",
    "rollup-plugin-terser": "^7.0.2"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  }
}
