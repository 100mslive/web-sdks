{
  "name": "@100mslive/react-icons",
  "source": "src/index.tsx",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "version": "0.0.3",
  "author": "100ms",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "svg-build": "svgr --icon --typescript --replace-attr-values '#fff=currentColor' --svg-props width='24px' --svg-props height='24px' -d src assets && yarn format",
    "build:only": "yarn svg-build && node ../../scripts/build",
    "types": "tsc -p tsconfig.build.json",
    "build": "yarn build:only && yarn types",
    "format": "prettier -w src/**",
    "lint": "eslint",
    "lint:fix": "eslint src/** --fix"
  },
  "devDependencies": {
    "@svgr/cli": "^5.5.0",
    "@types/react": "^17.0.38",
    "react": "^17.0.2"
  }
}
