    "rollup": "^2.70.1",
    "rollup-plugin-esbuild": "^4.9.1",
    "rollup-plugin-terser": "^7.0.2"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "dependencies": {
    "@100mslive/hms-video-store": "0.2.94",
    "react-intersection-observer": "^8.33.1",
